[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/berkyildiz3
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/berkyildiz3
	label-Verified = -1..+1 group user/berkyildiz3
	push = group user/berkyildiz3
	read = group user/berkyildiz3
	rebase = group user/berkyildiz3
	removeReviewer = group user/berkyildiz3
	submit = group user/berkyildiz3
[access "refs/heads/*"]
	create = group user/berkyildiz3
	push = +force group user/berkyildiz3
	pushMerge = group user/berkyildiz3
	read = group user/berkyildiz3
[access "refs/tags/*"]
	push = group user/berkyildiz3
	push = +force group user/berkyildiz3
